{
    "manifest_version": 2,
    "name": "Click to Duplicate",
    "version": "1.0",
    "author": "Vladimir Barbarosh <vladimir.barbarosh@gmail.com>",
    "description": "Click Click Click to Duplicate Duplicate Duplicate",
    "homepage_url": "https://github.com/vbarbarosh/chrome-ext-clickdup",
    "background": {
        "persistent": false,
        "scripts": ["background.js"]
    },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "64": "icons/icon64.png",
        "128": "icons/icon128.png"
    },
    "browser_action": {
        "default_title": "Click to Duplicate"
    },
    "permissions": [
        "activeTab"
    ]
}
